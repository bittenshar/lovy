							"listen": "test",
							"script": {
								"exec": [
									"pm.test('Status code is 201', function () {",
									"    pm.response.to.have.status(201);",
									"});",
									"",
									"pm.test('Response has success status', function () {",
									"    const jsonData = pm.response.json();",
									"    pm.expect(jsonData.status).to.eql('success');",
									"});",
									"",
									"pm.test('Response contains attendance record', function () {",
									"    const jsonData = pm.response.json();",
									"    pm.expect(jsonData.data).to.be.an('object');",
									"    pm.expect(jsonData.data._id).to.exist;",
									"    pm.expect(jsonData.data.worker).to.exist;",
									"    pm.expect(jsonData.data.job).to.exist;",
									"});",
									"",
									"// Save record ID for other tests",
									"if (pm.response.code === 201) {",
									"    const jsonData = pm.response.json();",
									"    pm.collectionVariables.set('recordId', jsonData.data._id);",
									"}"
								],
