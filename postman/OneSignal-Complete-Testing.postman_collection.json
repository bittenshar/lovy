        {
          "name": "Login - Worker",
          "event": [
            {
              "listen": "test",
              "script": {
                "exec": [
                  "if (pm.response.code === 200) {",
                  "    var jsonData = pm.response.json();",
                  "    pm.test('‚úÖ Login successful', function() {",
                  "        pm.expect(jsonData.status).to.equal('success');",
                  "        pm.expect(jsonData.data).to.have.property('token');",
                  "    });",
                  "    ",
                  "    if (jsonData.data && jsonData.data.token) {",
                  "        pm.environment.set('auth_token', jsonData.data.token);",
                  "        pm.environment.set('user_id', jsonData.data.user._id);",
                  "        console.log('‚úÖ Auth token saved: ' + jsonData.data.token.substring(0, 20) + '...');",
                  "        console.log('‚úÖ User ID saved: ' + jsonData.data.user._id);",
                  "    }",
                  "} else {",
                  "    pm.test('‚ùå Login failed', function() {",
                  "        pm.expect.fail('Login endpoint returned ' + pm.response.code);",
                  "    });",
                  "}"
                ]
              }
            }
          ],
          "request": {
            "method": "POST",
            "header": [
              {
                "key": "Content-Type",
                "value": "application/json",
                "type": "text"
